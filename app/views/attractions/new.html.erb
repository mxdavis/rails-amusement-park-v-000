<%= form_for Attraction.new do |f| %>
      Name
      <%= f.text_field :name %>
      Tickets
      <%= f.number_field :tickets %>
      Nausea Rating
      <%= f.number_field :nausea_rating %>
      Happiness Rating
      <%= f.number_field :happiness_rating %>
      Minimum Height
      <%= f.number_field :min_height %>
      <%= f.hidden_field :user_id, value: current_user.id %>
      <%= f.submit %>
    <% end %>